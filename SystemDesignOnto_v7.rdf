        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Through outgoing port p1, component C sends data the its next stop</rdfs:comment>
        <rdfs:label>outgoing port</rdfs:label>
        <AnnotationVocabulary:naturalLanguageDefinition xml:lang="en">An incoming port is an interaction point through which data, command, material, or energy can enter the owning block.</AnnotationVocabulary:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- http://www.pics.lgp.uttop.fr/ontologies/systemdesign#Port -->

    <owl:Class rdf:about="http://www.pics.lgp.uttop.fr/ontologies/systemdesign#Port">
