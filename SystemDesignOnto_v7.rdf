    <!-- http://www.pics.lgp.uttop.fr/ontologies/systemdesign#handlesMaterial -->

    <owl:ObjectProperty rdf:about="http://www.pics.lgp.uttop.fr/ontologies/systemdesign#handlesMaterial">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.pics.lgp.uttop.fr/ontologies/systemdesign#Port"/>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000040"/>
        <rdfs:label xml:lang="en">handles material</rdfs:label>
        <skos:definition xml:lang="en">x handles material y = there exists process boudary z, port x participates in process boundary z, and material entity y participates in process boundary z.</skos:definition>
        <AnnotationVocabulary:firstOrderLogicAxiom>handleMaterial(x,y) ↔ Port(x) ∧ MaterialEntity(y) ∧ ∃z(ProcessBoundary(z) participatesIn(x,z) ∧ participatesIn(y,z)</AnnotationVocabulary:firstOrderLogicAxiom>
    </owl:ObjectProperty>
    


    <!-- http://www.pics.lgp.uttop.fr/ontologies/systemdesign#hasFunctionPartAtAllTimes -->

    <owl:ObjectProperty rdf:about="http://www.pics.lgp.uttop.fr/ontologies/systemdesign#hasFunctionPartAtAllTimes">
        <rdfs:subPropertyOf rdf:resource="http://www.pics.lgp.uttop.fr/ontologies/systemdesign#hasFunctionPartAtSomeTime"/>
        <rdfs:label>has function part at all times</rdfs:label>
        <AnnotationVocabulary:naturalLanguageDefinition xml:lang="en">p has function part c at all times =Def for all times t, p exists at t implies (p is a function, c is a function, and c functions to make p functions some way at t)</AnnotationVocabulary:naturalLanguageDefinition>
    </owl:ObjectProperty>
    


